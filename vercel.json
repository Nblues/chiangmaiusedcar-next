{
  "framework": "nextjs",
  "env": {
    "SHOPIFY_DOMAIN": "kn-goodcar.com",
    "SHOPIFY_STOREFRONT_TOKEN": "bb70cb008199a94b83c98df0e45ada67",
    "NEXT_PUBLIC_RECAPTCHA_SITE_KEY": "6LevqZkrAAAAAPAsPmJk_qUo6kGlvGsy2xdvvL1A",
    "RECAPTCHA_SECRET_KEY": "6LevqZkrAAAAAKl6fMzyeYyHFOEMLATFnilQCj_U",
    "NEXT_PUBLIC_EMAILJS_SERVICE_ID": "service_qlcksif",
    "NEXT_PUBLIC_EMAILJS_TEMPLATE_ID": "template_zd6e3f6",
    "NEXT_PUBLIC_EMAILJS_PUBLIC_KEY": "P3wnNJB_Y_PddrdBJ",
    "SITE_URL": "https://chiangmaiusedcar.com"
  },
  "alias": ["chiangmaiusedcar.com", "www.chiangmaiusedcar.com"],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:all*(css|js|woff2|woff|ttf|otf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:all*(jpg|jpeg|png|webp|avif|gif|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(sitemap.xml|robots.txt|favicon.ico|manifest.json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ]
}
