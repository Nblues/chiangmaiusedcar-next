{
  "name": "chiangmaiusedcar-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/check-no-html-imports.cjs",
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:memory": "set NODE_OPTIONS=--max-old-space-size=4096 && next build",
    "start": "next start -p 3000",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postbuild-disabled": "next-sitemap; node scripts/generate-image-sitemap.js || true; node scripts/generate-pagination-sitemap.js || true",
    "generate-sitemap": "node scripts/generate-image-sitemap.js && node scripts/generate-pagination-sitemap.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "build:analyze": "cross-env ANALYZE=true next build",
    "production": "npm run build && npm run start"
  },
  "dependencies": {
    "@fontsource/prompt": "^5.2.6",
    "@next/third-parties": "^14.2.5",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.11.0",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "emailjs-com": "^3.2.0",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "next": "14.2.5",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "speakeasy": "^2.0.0",
    "sweetalert2": "^11.22.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.10",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "cssnano": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.10",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0"
}
